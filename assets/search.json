---
layout: none
---

[
  {% for post in site.posts %}
    {% if post.lang == site.active_lang or post.lang == nil and site.active_lang == site.default_lang %}
    {
      "title"    : "{{ post.title | escape }}",
      "description" : "{{ post.description | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
